{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [{
      "source": "**",
      "function": "api"
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "/",
      "headers": [{
        "key": "Link",
        "value": "<./css/main.css>;rel=preload;as=style"
      }]
    }]
  }
}
